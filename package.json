{
  "name": "iqbe",
  "version": "2.0.0",
  "description": "A powerful tool to enhance your quiz skills more fun and effective. ",
  "workspaces": [
    "packages/frontend",
    "packages/backend"
  ],
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node ./scripts/build.mjs",
    "start": "cd packages/backend/dist && node index.js",
    "clean": "node ./scripts/clean.mjs",
    "dev": "node ./scripts/dev.mjs",
    "migrate:create": "cd packages/backend && npm run migrate:create",
    "migrate:exe": "cd packages/backend && npm run migrate:exe",
    "migrate:status": "cd packages/backend && npm run migrate:status"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/woodnext/iqbe-dev.git"
  },
